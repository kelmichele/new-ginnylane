<% content_for :title do %>
Daily Specials at Orange Beach, AL Seafood Restaurant 
<% end %>
<% content_for :description do %>
Keep up with Ginny Lane's specials, &amp; never miss one of our great deals.
<% end %>


<div class="specialsPage multi">
  <section class="simplePt red bmargin0">
    <div class="inner">
      <h1>Check Out Our New Specials!</h1>
      <p class="instr">Click an image below to view larger version.</p>
    </div>
  </section>

  <div class="foodWrap">
    <%# <= image_tag('bannerHappy.jpg', class: 'img-responsive', alt: "Daily Happy Hour at Ginny Lane") > %>
    <div class="insideAlt fwInner">

      <div class="fullSec mtop hasSticky">
        <div class="item combo i1 boxShadow">
          <%= link_to "HappyHour.pdf", target: '_blank', class: 'menu-link menuBg' do %>
            <%= image_tag('happyFood.png', class: 'img-responsive', alt: "Ginny Lane Happy Hour Food") %>
            <%= image_tag('happyDrinks.png', class: 'img-responsive', alt: "Ginny Lane Happy Hour Drinks") %>
          <% end %>
        </div>

        <div class="item combo i2 stickyTn">
          <%= link_to "WeeklySpecials.pdf", target: '_blank', class: 'menu-link menuBg' do %>
            <%= image_tag('weeklySpecials.png', class: 'img-responsive', alt: "Ginny Lane Happy Hour, Orange Beach, AL") %>
          <% end %>
        </div>
      </div>
    </div>

    <div class="fullBorder"></div>
  </div>
</div>