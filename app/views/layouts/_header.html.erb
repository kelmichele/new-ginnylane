<div id="h-wrap">
  <header class="header">
    <div id="top-bar-wrapper" class="clearfix">
      <div class="col-md-4 xs-l">
        <i class="fa fa-phone"></i>
        <a class="tel" href="tel:+12512246500">251-224-6500 ext. 1</a>
      </div>

      <div class="col-md-4">
      </div>

      <div class="col-md-4 Xcol-md-offset-4 xs-f">
        <ul id="social-links">
          <li>
            <%= link_to 'https://www.facebook.com/pages/Ginny-Lane-Bar-Grill/159850265732', target: '_blank' do %>
              <i class="fa fa-facebook"></i>
            <% end %>
          </li>

          <li>
            <%= link_to 'https://plus.google.com/117900168660588676207/about?hl=en', target: '_blank' do %>
              <i class="fa fa-google-plus"></i>
            <% end %>
          </li>

          <li>
            <%= link_to 'https://twitter.com/ginnylaneob', target: '_blank' do %>
              <i class="fa fa-twitter"></i>
            <% end %>
          </li>

          <li>
            <a href="https://www.instagram.com/ginnylanebarandgrill/" target="_blank"><i class="fa fa-instagram"></i></a>
          </li>
        </ul>
      </div>
    </div>

    <div class="main-header">
      <div class="col-md-3 logo-block">
        <%= link_to(image_tag('gl-logo.jpg', alt: 'Ginny Lane Bar and Grill Logo', class: 'img-responsive logo-img'), root_path ) %>
      </div>

      <div class="col-md-9 mega-tab">
        <ul class="megamenu-parent menu-zoom hidden-xs">
          <%= render 'shared/nav' %>
        </ul>
      </div>
          <!-- http://www.opentable.com/ginny-lane-bar-and-grill-reservations-orange-beach?restref=144784 -->
